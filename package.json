{
  "author": "Anteneh Addis",
  "dependencies": {
    "@cmsgov/design-system-core": "latest",
    "bluebird": "3.5.1",
    "body-parser": "1.18.3",
    "bunyan": "1.8.12",
    "celebrate": "8.0.1",
    "compression": "1.7.3",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "express-fileupload": "1.0.0",
    "http-status": "1.2.0",
    "joi": "13.6.0",
    "pg": "7.4.3",
    "sequelize": "4.38.0"
  },
  "description": "CMS Cloud Modernization App",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "9.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "eslint": "5.5.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.14.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
    "jest": "23.5.0",
    "nodemon": "1.18.4",
    "sequelize-cli": "4.1.1",
    "sinon": "6.1.5"
  },
  "name": "cms-cloud-template-backend-app",
  "scripts": {
    "dev": "NODE_ENV=localhost babel-node ./src/bin/www --inspect=5101",
    "dist": "node_modules/.bin/gulp",
    "docker:build:app": "docker build -t cms-cloud-app .",
    "docker:build:db": "docker build -f Dockerfile-db -t cms-cloud-db .",
    "docker:down": "docker-compose down",
    "docker:test": "docker exec -it cms-cloud-app yarn test",
    "docker:up": "docker-compose up -d; docker logs -f cms-cloud-app",
    "lint": "eslint ./testapp/src",
    "lint:fix": "eslint ./testapp/src --fix",
    "local": "nodemon --exec yarn dev | bunyan -o short",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate-seed-start": "yarn migrate && yarn seed && yarn start",
    "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo",
    "seed": "node_modules/.bin/sequelize db:seed:all",
    "seed:undo": "node_modules/.bin/sequelize db:seed:undo:all",
    "start": "babel-node ./src/bin/www",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "version": "1.0.0"
}
